<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="fu.android_playground">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Compose"
        tools:targetApi="31">
        <profileable
            android:shell="true"
            tools:targetApi="q" />

        <activity
            android:name=".view.main.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Compose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.column.ColumnActivity"></activity>
        <activity android:name=".view.row.RowActivity"></activity>
        <activity android:name=".view.constraintlayout.ConstraintLayoutActivity"></activity>
        <activity android:name=".view.login.LoginActivity"></activity>
    </application>

</manifest>